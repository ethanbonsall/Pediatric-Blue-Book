{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:load:login": "dotenv -e .env.local -- artillery run load-test-login.yml",
    "test:load:login:report": "dotenv -e .env.local -- artillery run --output login-report.json load-test-login.yml && artillery report login-report.json",
    "test:load:lookup": "dotenv -e .env.local -- artillery run load-test-lookup.yml",
    "test:load:lookup:report": "dotenv -e .env.local -- artillery run --output lookup-report.json load-test-lookup.yml && artillery report lookup-report.json",
    "test:load:both": "npm run test:load:login && npm run test:load:lookup"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.48.1",
    "@tanstack/react-query": "^5.66.9",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
